<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hermawan.mahasiswaonline" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".splashscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".FormMahasiswa" >
        </activity>
        <activity android:name=".DetailMahasiswa" >
        </activity>
        <activity
            android:name=".FormKos"
            android:label="@string/title_activity_form_kos" >
        </activity>
        <activity
            android:name=".DetailKos"
            android:label="@string/title_activity_detail_kos" >
        </activity>
    </application>

</manifest>
